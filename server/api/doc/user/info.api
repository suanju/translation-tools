syntax = "v1"

info(
    desc: "用户信息"
)


type (
    UserInfoReq {
    }

    UserInfoResp {
        Email string `json:"email"`
    }
)
@server(
    jwt: Auth
    group: user/info
    prefix: /user
)

service api {
    @handler UserInfo
    get /get_user_info (UserInfoReq) returns (UserInfoResp)
}
